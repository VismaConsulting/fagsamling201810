<div class="text-muted">
    <div class="container-fluid">
        <div class="album">
            <div class="container">
                <h2 id="myprojects">My projects</h2>
                <div class='row'>
                    <% page.posts.each(function(post){ %>
                    <div class="col-sm">
                        <div class="card">
                            <div class="hovereffect">
                                <a href="<%= url_for(post.path) %>">
                                    <img class="img-responsive"
                                         src="<%- post.thumbnail %>"
                                         alt="<%= post.title %> - <%= date(post.date, config.date_format) %>">
                                    <div class="overlay">
                                        <h2><%= post.title %></h2>
                                    </div>
                                </a>
                            </div>
                        </div>
                    </div>
                    <% }) %>
                </div>
            </div>
        </div>
    </div>

    <div class="container">
        <h2 id="aboutme">Who Am I?</h2>
    </div>

    <div class="container">
        <h2 id="contact">Contact Me</h2>
    </div>
</div>